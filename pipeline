pipeline {
    agent any

    stages {
        stage(git) {
           steps {
             sh 'yum install git -y'
          }
      }
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
